#include<bits/stdc++.h>
using namespace std;
int binaryExponentiation(int x,int n)
{
    if(n==0)
        return 1;
    long res=binaryExponentiation(x,n/2);
   if(n%2 == 0)        //n is even
        return res*res;
    else                             //n is odd
        return x*res*res;
}
int main()
{
   
  int x,n;
  cin>>x>>n;
  cout<<"power of x^n is:"<<binaryExponentiation(x,n);
   
}
