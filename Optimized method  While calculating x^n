#include<bits/stdc++.h>
using namespace std;
int binaryExponentiation(int x,int n)
{
    if(n==0)
        return 1;
    else if(n%2 == 0)        //n is even
        return binaryExponentiation(x*x,n/2);
    else                             //n is odd
        return x*binaryExponentiation(x*x,(n-1)/2);
}
int main()
{
   
  int x,n;
  cin>>x>>n;
  cout<<"power of x^n is:"<<binaryExponentiation(x,n);
   
}
