#include<bits/stdc++.h>
using namespace std;
int binaryExponenItera(int a,int b){
    int ans=1;
    while(b){
        if(b&1){
            ans=ans*a;
        }
        a=a*a;
        b>>=1;
        
    }
    
    return ans;
    
    
}
int main()
{
   
  int x,n;
  cin>>x>>n;
  cout<<"power of x^n is:"<<binaryExponenItera(x,n);
   
}
